<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sns.db.BoardMapper">



	<!-- 게시판에 내용을 추가하는 sql 문장 -->
	<insert id="upload" parameterType="sns.model.BoardDTO">
		insert into content values(#{member_id},null, num_board.nextval,#{title},#{content},sysdate, #{attachment},#{like_count} )
	</insert>

</mapper>